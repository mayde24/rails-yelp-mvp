<h1>Edit a restaurant</h1>

<%= form_for(@restaurants) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :category %>
  <%= f.select :category, ["chinese", "italian", "japanese", "french", "belgian"]%>

  <%= f.submit %>
<% end %>
